{
    "AzureKeyVault": {
        "ApplicationId": "OVERRIDE_KEYVAULT_APP_ID",
        "ApplicationSecret": "OVERRIDE_KEYVAULT_APP_SECRET",
        "Url": "OVERRIDE_KEYVAULT_URL"
    },
    "ConnectionStrings": {
        "RenewalDb": "atlanta-dbconnstring-renewals"
    },
    "Products": [
        {
            "ApiVersions": [ "1" ],
            "Context": "-;CN;Carole Nash Renewals;{0}",
            "Name": "CarolenashRenewalPortal"
        }
    ],
    "PaymentServiceEndpointUrl": "http://atlanta-svc-payment/api/{0}",
    "PingServiceUrls": {
        "IdentityServer": "http://atlanta-svc-identity-server-{environment}/ping-health-check"
    },
    "Serilog": {
        "Enrich": [
            "WithAtlantaLogEnricher"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.AspNetCore.Authentication": "Information",
                "Microsoft.Extensions.Diagnostics.HealthChecks": "Error",
                "System": "Warning"
            }
        },
        "Properties": {
            "ServiceName": "Renewal"
        },
        "Using": [
            "Renewal.Application",
            "Serilog.Sinks.Http"
        ],
        "WriteTo": [
            {
                "Args": {
                    "bufferPathFormat": "C:\\logs\\elk\\atlanta-renewal-{Date}.json",
                    "requestUri": "OVERRIDE_LOGGING_URL"
                },
                "Name": "DurableHttpUsingTimeRolledBuffers"
            }
        ]
    }
}
